    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "lighthouse": "^8.5.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "pg": "^8.7.1",
    "puppeteer": "^10.4.0",
    "sql-template-strings": "^2.2.2",
    "uuid": "^8.3.2",
    "wait-on": "^6.0.0",
    "winston": "^3.3.3"
