    "format": "web-scripts format",
    "commit": "web-scripts commit",
    "release": "web-scripts release",
    "prepare": "web-scripts audit && husky install"
  },
  "devDependencies": {
    "@spotify/eslint-config-oss": "^1.0.2",
    "@spotify/web-scripts": "^12.0.0",
    "@types/async-retry": "^1.4.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.9.6",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@types/wait-on": "^5.3.1",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "get-port": "^5.1.1",
    "husky": "^7.0.0",
    "node-duration": "^2.0.0",
    "nodemon": "^2.0.13",
    "supertest": "^6.1.6",
    "testcontainers": "^7.20.6",
    "ts-node": "^10.2.1"
